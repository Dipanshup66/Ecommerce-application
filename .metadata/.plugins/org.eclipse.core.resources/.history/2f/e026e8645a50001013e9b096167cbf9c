spring:
 application:
   name: api-gateway
 cloud:
   gateway:
     routes:
       - id: user-service
         uri: http://localhost:8081
         predicates:
           - Path=/users/**
server:
 port: 8080
eureka:
 client:
   service-url:
     defaultZone: http://localhost:8761/eureka
   register-with-eureka: true
   fetch-registry: true